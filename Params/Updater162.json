{
    "net":"PointNet",
    "net-args":{
        "layer_sets":[[500,500],[500,500],[500,500,500,1024]],
        "input_size":67,
        "activation":"SELU"
    },
    "encoder":"MLP",
    "encoder-args":{
        "layers":[1024,64],
        "batch_norm":"BatchNorm1d",
        "activation":"SELU"
    },
    "updater":"Updater",
    "updater-args":{
        "constrain_amp":true
    },
    "start-epochs":0,
    "epochs":50,
    "train":["Train-100-5000-4A.pth"],
    "test":["Test-100-100-4A.pth"],
    "optimiser":"NAdam",
    "optimiser-args":{"lr":1e-4},
    "loss-function":"mse_loss",
    "supervised":true,
    "batch":100,
    "clip":true,
    "scheduler":"CyclicLR",
    "scheduler-args":{"base_lr":1e-5,
                        "max_lr":1e-4,
                        "step_size_up":5,
                        "cycle_momentum":false
                    },
    "clip-args":{"max_norm":1e7},
    "log-grad":false,
    "phase-reg-fun":"mse_loss",
    "phase-reg-lambda":1e4,
    "pressure-reg-fun":"mse_loss",
    "pressure-reg-lambda":1e-2
}