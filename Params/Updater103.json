{
    "net":"PointNet",
    "net-args":{
        "layer_sets":[[32],[32],[32,32,1024]],
        "batch_norm":"BatchNorm1d",
        "input_size":13,
        "activation":"SELU"
    },
    "encoder":"MLP",
    "encoder-args":{
        "layers":[1024,10],
        "batch_norm":"BatchNorm1d",
        "activation":"SELU"
    },
    "updater":"Updater",
    "updater-args":{
        "constrain_amp":true
    },
    "start-epochs":0,
    "epochs":50,
    "train":["Train-10-70000-4A.pth"],
    "test":["Test-10-1000-4A.pth"],
    "optimiser":"NAdam",
    "optimiser-args":{"lr":1e-4},
    "loss-function":"mse_loss",
    "supervised":true,
    "batch":32,
    "scheduler-args":{"base_lr":1e-5,
                        "max_lr":1e-3,
                        "step_size_up":5,
                        "cycle_momentum":false
                    },
    "clip":true,
    "clip-args":{"max_norm":1e7},
    "log-grad":false
}