{
    "net":"ResPointNet",
    "net-args":{
        "layer_sets":[[48,48,48,48],[48,48,48,48],[48,48,48,1024]],
        "input_size":35,
        "activation":"SELU"
    },
    "encoder":"MLP",
    "encoder-args":{
        "layers":[1024,32],
        "batch_norm":"BatchNorm1d",
        "activation":"SELU"
    },
    "updater":"Updater",
    "updater-args":{
        "constrain_amp":true
    },
    "start-epochs":0,
    "epochs":50,
    "train":["Train-100-10-4A.pth"],
    "test":["Test-100-5-4A.pth"],
    "optimiser":"NAdam",
    "optimiser-args":{"lr":1e-2},
    "loss-function":"mse_loss",
    "supervised":true,
    "batch":5,
    "clip":true,
    "clip-args":{"max_norm":1e9},
    "log-grad":false,
    "norm-loss":true
}